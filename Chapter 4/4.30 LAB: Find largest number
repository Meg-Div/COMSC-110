#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {

   vector <int> integers;
   int number;
   
   while (cin >> number && number > -1) {
      integers.push_back(number);
   }
   
   sort(integers.begin(), integers.end());
   
   cout << integers.back() << endl;

   return 0;
}
