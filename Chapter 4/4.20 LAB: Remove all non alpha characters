#include <iostream>
#include <locale>       
using namespace std;

int main() {
   
   string userString, finalString;
   char character;
   
   getline(cin, userString);

   for (size_t i = 0; i < userString.size(); ++i) {
      character = userString.at(i);
      
      if (isalpha(character)) {
         finalString.push_back(character);
      }
}
      cout << finalString << endl;

   return 0;
}
