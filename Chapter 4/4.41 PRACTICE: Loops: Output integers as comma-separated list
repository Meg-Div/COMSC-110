#include <iostream>
#include <string>
using namespace std;

int main() {

   int numOfInts;
   string currNum, commaList;
   
   cin >> numOfInts;
   
   for (int i = 0; i < numOfInts; ++i) {
      cin >> currNum;
      commaList.append(currNum);
      if (i == (numOfInts - 1)) {
         commaList.append(".");
      } 
      else {
         commaList.append(", ");
      }
   }
   
   cout << commaList << endl;

   return 0;
}
