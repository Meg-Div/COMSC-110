#include <iostream>
#include <cstdlib>
#include <iomanip>

using namespace std;

int FeetToSteps(double userFeet) 
{
   
   int steps = (userFeet / 2.5);
   
   cout << steps << endl;
   
   return steps;
}

int main() 
{
   double feet;
   string buf;
   
   cin >> buf;
   
   feet = atof(buf.c_str());
   
   FeetToSteps(feet);
   
   return 0;
}
