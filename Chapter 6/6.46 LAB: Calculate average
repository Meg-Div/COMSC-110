#include <iostream>
#include <vector>

using namespace std;

double CalcAverage(vector<int> nums) 
{
   double sum = 0; 
   
   for (int num : nums) sum += num;
   
   return sum / nums.size();  
}

int main() 
{
   vector<int> nums = {1, 2, 3, 4, 5};
   
   cout << CalcAverage(nums) << endl;
   
   return 0;
}
